<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginEnd="8dp"
    android:layout_marginStart="8dp"
    android:layout_marginTop="1dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardCornerRadius="10dp"

    >

    <RelativeLayout
        android:id="@+id/subject_overview_activity_relative_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:baselineAligned="false"
        android:onClick="onClickStartModuleActivity">


            <CheckBox
                android:id="@+id/subject_overview_checkbox"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                />

            <TextView
                android:id="@+id/subject_overview_module_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Module"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:layout_toEndOf="@+id/subject_overview_checkbox"/>



    </RelativeLayout>



</androidx.cardview.widget.CardView>








